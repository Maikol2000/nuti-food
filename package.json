  "scripts": {
    "dev": "npm run && webpack-dev-server --mode development",
    "build-prod": "npm run clean && npm run && webpack --mode production && npm run build:pwa:web",
    "clean": "rimraf dist/prod",
    "build-dev": "npm run clean-dev && npm run && webpack --mode none && npm run build:pwa:web:dev",
    "clean-dev": "rimraf dist/build",
    "start": "serve -s dist",
    "extract": "formatjs extract",
    "build:pwa:web": "webpack --config ./service-worker/webpack.prod.config.js --progress --colors && node workbox-build-inject-prod.js && rimraf dist/prod/service-worker-src.js",
    "build:pwa:web:dev": "webpack --config ./service-worker/webpack.dev.config.js --progress --colors && node workbox-build-inject.js && rimraf dist/build/service-worker-src.js"
